{
  "name": "d3-geo",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky",
    "dev": "vite",
    "preview": "vite preview",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:ci": "pnpm run lint --cache --cache-strategy content --cache-location ~/.cache/eslint/d3-geo",
    "build": "tsc --project tsconfig.build.json && vite build",
    "test": "vitest run",
    "coverage": "vitest run --coverage"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{js,json,md,yml}": "prettier --write"
  },
  "dependencies": {
    "d3-geo": "^3.1.1",
    "d3-selection": "^3.0.0"
  },
  "devDependencies": {
    "@types/d3-geo": "^3.1.0",
    "@types/d3-selection": "^3.0.10",
    "@typescript-eslint/eslint-plugin": "^7.5.0",
    "@typescript-eslint/parser": "^7.5.0",
    "@vitest/coverage-v8": "^1.4.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^9.0.11",
    "jsdom": "^24.0.0",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "typescript": "^5.4.4",
    "vite": "^5.2.8",
    "vitest": "^1.4.0"
  },
  "engines": {
    "node": "^20.11",
    "pnpm": "^8"
  }
}
